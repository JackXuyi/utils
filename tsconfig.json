{
  "compilerOptions": {
    "module": "esnext",
    "target": "esnext",
    "types": ["node"],
    "declaration": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@typings/*": ["typings/*"],
      "@utils/*": ["src/utils/*"],
      "@service/*": ["src/service/*"],
      "@constant/*": ["src/constant/*"]
    }
  },
  "skipLibCheck": true,
  "skipDefaultLibCheck": true
}
